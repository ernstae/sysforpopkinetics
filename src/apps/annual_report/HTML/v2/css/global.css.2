/* These are my global styling rules */

BODY    {
                background-color: #FFFFFF;
                color: #000000;
                font-family: verdana, helvetica, arial;
                font-size:10pt;
                font-style:normal;
                font-weight:500;
        }

A:link  {
                color: #323232;
        }

A:visited {
                color: #232323;
        }

A:active {
                color: #FF0000;
        }

