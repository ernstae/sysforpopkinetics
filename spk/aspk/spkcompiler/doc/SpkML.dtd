<!-- <submission> Date and time which the request was submitted and its originator information -->
<!ELEMENT submission (owner | date | time)>

<!-- <release> Date and time at which the result was generated and its originator information -->
<!ELEMENT release (owner? | date | time)>

<!-- <owner> Owner of the SpkML instance. -->
<!ELEMENT owner (login | last_name? | first_name? | description?)>
<!ELEMENT login      (#PCDATA)><!-- <login> Login info -->
<!ELEMENT last_name  (#PCDATA)><!-- <last_name> Last name -->
<!ELEMENT first_name (#PCDATA)><!-- <first_name> First name -->

<!--  <description>  Description of the parent element. -->
<!ELEMENT description (#PCDATA)>

<!-- <date> Month-Date-Year. -->
<!ELEMENT date (mm | dd | yyyy)>
<!ELEMENT mm   (#PCDATA)><!-- <mm> Month in 2 digits -->
<!ELEMENT dd   (#PCDATA)><!-- <dd> Date in 2 digits -->
<!ELEMENT yyyy (#PCDATA)><!-- <yyyy> Year in 4 digits -->

<!-- <time> Hour-Minutes-Seconds. -->
<!ELEMENT time (hr | min | sec)>
<!ELEMENT hr   (#PCDATA)><!-- <hr> Hour in 2 digits -->
<!ELEMENT min  (#PCDATA)><!-- <min> Minutes in 2 digits -->
<!ELEMENT sec  (#PCDATA)><!-- <sec> Seconds in 2 digits -->
