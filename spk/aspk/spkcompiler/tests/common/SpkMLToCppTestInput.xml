<spkinml>
	<content spkinml_ver="1.0" client="nonmem" client_ver="1.1" analysis="population">
        This SpkInML document is an input to NonmemCompilerTest
	</content>
<driver>
	<theta length="3">
		<in>
			<value fixed="yes">0.1</value>
			<value fixed="no">0.1</value>
			<value>0.1</value>
                </in>
                <low>
                        <value>-0.1</value>
                        <value>-0.1</value>
                        <value>-0.1</value>
                </low>
                <up> 
                        <value>1.0</value>
                        <value>1.0</value>
                        <value>1.0</value>
                </up>
        </theta>
        <sigma dimension="1" struct="diagonal">
                <in>
                        <value>0.3</value>
                </in>
        </sigma>
        <omega dimension="3" struct="block">
                <in>
                        <value fixed="yes">0.1</value>
                        <value fixed="no">0.1</value>
                        <value>0.1</value>
                        <value fixed="yes">0.1</value>
                        <value>0.1</value>
                        <value>0.1</value>
		</in>
	</omega>
	<eta length="3">
		<in>
			<value fixed="no">0.0</value>
			<value fixed="yes">0.0</value>
			<value>0.0</value>
		</in>
	</eta>
	<pop_opt approximation="fo"
                 pop_size="12"
                 epsilon="0.001"
                 mitr="450"
                 trace="4"
                 restart="yes"
                 par_out="yes"
                 obj_out="yes"
                 deriv1_out="yes"
                 deriv2_out="no"/>
        
        <ind_opt epsilon="0.01"
                 mitr="100" 
                 trace="0" 
                 restart="yes" 
                 par_out="yes"
                 obj_out="yes"
                 deriv1_out="yes"
                 deriv2_out="no"/>

	<pop_stat stderror="yes" correlation="yes" cov="yes" coefficient="yes" confidence="yes"/>

        <ind_stat stderror="no"  correlation="no"  cov="no"  coefficient="no"  confidence="no"/>

</driver>

<model base="advan2"> 
	<pk>
 		if (DOSE.NE.0) THEN
                   DS=DOSE*WT
                   W=WT
                ENDIF
                KA=THETA(1)+ETA(1)
                KE=THETA(2)+ETA(2)
                CL=THETA(3)*W+ETA(3)
                D=EXP(-KE*TIME)-EXP(-KA*TIME)
                E=CL*(KA-KE)
                F=DS*KE*KA/E*D
 	</pk>

	<error>
                Y=F+EPS(1)
	</error>

</model>

