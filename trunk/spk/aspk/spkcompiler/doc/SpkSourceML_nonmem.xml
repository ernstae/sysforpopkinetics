<?xml version="1.0"?>
<source>
   <nonmem>
       <constraint>
          <pop_analysis is_estimation="yes"
                        approximation="fo"
                        pop_size="12">
             <description>CONTROL 4 + THEOPHYLLINE example</description>
             <data_labels>
                <label name="ID"/>
                <label name="DOSE" synonym="AMT"/>
                <label name="TIME"/>
                <label name="CL" synonym="DV"/>
                <label name="WT"/>
             </data_labels>
             <theta length="3">
                <low>
                   <double value="0.1"/>
                   <double value=".008"/>
                   <double value=".004"/>
                </low>
                <in>
                   <double value="3."/>
                   <double value=".08"/>
                   <double value="0.04"/>
                </in>
                <up>
                   <double value="5"/>
                   <double value=".5"/>
                   <double value="0.9"/>
                </up>
             </theta>
             <omega struct="block" dimension="3">
                <in>
                   <double value="6"/>
                   <double value="0.005"/>
                   <double value="0.0002"/>
                   <double value=".3"/>
                   <double value=".006"/>
                   <double value=".4"/>
                </in>
             </omega>
             <sigma struct="diagonal" dimension="1">
                <in>
                   <double value=".4"/>
                </in>   
             </sigma>
             <eta length="3">
                <in>
                   <double value="0.0"/>
                   <double value="0.0"/>
                   <double value="0.0"/>
                </in>
             </eta>
             <pop_stat is_stderror_out="yes"
                       is_covariance_out="yes"
                       is_correlation_out="yes"
                       covariance_form="r"/>
             <ind_stat is_stderror_out="no"
                       is_covariance_out="yes"
                       is_correlation_out="no"/>
          </pop_analysis>
       </constraint>
       <model>
          <description>Two compartmental model</description>
             <pred>
;THETA(1)=MEAN ABSORPTION RATE CONSTANT (1/HR)
;THETA(2)=MEAN ELIMINATION RATE CONSTANT (1/HR)
;THETA(3)=SLOPE OF CLEARANCE VS WEIGHT RELATIONSHIP (LITERS/HR/kg)
;DOSE=WT-ADJUSTED DOSE (mg/kg)
;DS=NON-WT-ADJUSTED DOSE (mg)
   IF (DOSE.NE.0) THEN
      DS=DOSE*WT
      W=WT
   ENDIF
   KA=THETA(1) + ETA(1)
   KE=THETA(2) + ETA(2)
   CL=THETA(3) * WT + ETA(3)
   D=EXP(-KE*TIME)-EXP(-KA*TIME)
   E=CL*(KA-KE)
   F=DS*KE*KA/E*D
   Y=F+EPS(1)
            </pred>
          </model>
       <presentation>
          <table header="every" process="estimation">
             <column label="ID"/>
             <column label="DOSE"/>
             <column label="WT"/>
             <column label="TIME"/>
          </table>
          <scatterplot process="estimation" y0_line="show" unit_slope="yes">
             <y label="RES"/>
             <y label="WRES"/>
             <x label="TIME"/>
             <by label="ID"/>
          </scatterplot>
       </presentation>
   </nonmem>
</source>
